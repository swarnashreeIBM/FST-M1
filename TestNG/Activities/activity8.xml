<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="FSTTests" thread-count="4" parallel="classes">
	<!-- Comment out groups to run everything -->
	<groups>
		<run>
			<include name="HeaderTests" />
		</run>
	</groups>
	<parameter name="username" value="admin" />
	<parameter name="password" value="password" />
	<test name="FSTActivities">
		<classes>
			<class name="fst_testNG.Activity1" />
			<class name="fst_testNG.Activity2" />
			<class name="fst_testNG.Activity3" />
			<class name="fst_testNG.DemoOne" />
			<class name="fst_testNG.DemoTwo" />
			<class name="fst_testNG.Activity5" />
			<class name="fst_testNG.Activity6" />
			<class name="fst_testNG.Activity7" />
		</classes>
	</test>
</suite>